      "post": {
        "summary": "Filter by category",
        "description": "Filter documents and persons by category.",
        "tags": [
          "Filter"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "schema": {
              "type": "object",
              "properties": {
                "category": {
                  "type": "string",
                  "description": "The category to filter by.",
                  "enum": [
                    "LostDocument",
                    "MissingPerson"
                  ]
                }
              }
            },
            "required": true,
            "description": "Filter input"
          }
        ],
        "responses": {
          "200": {
            "description": "Filter results returned successfully.",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "results": {
                  "type": "integer",
                  "description": "Number of results returned"
                },
                "data": {
                  "type": "array",
                  "items": {
                    "type": "object"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad request. Category is required or invalid."
          },
          "500": {
            "description": "Internal server error."
          }
        }
      }
    },
